                        <a href="{{product.metafields.meta.video}}"><span>{{ 'products.product.video' | t }}</span></a>
                        </div>
                     </div>
                  {%- endif -%}{% endcomment %}
  
                  {% include 'mtnops_product_video' %}

               </div>
